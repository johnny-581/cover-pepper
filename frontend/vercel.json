{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/app/:path*",
            "destination": "/"
        },
        {
            "source": "/((?!assets|favicon.ico|robots.txt|manifest.webmanifest).*)",
            "destination": "/"
        }
    ]
}